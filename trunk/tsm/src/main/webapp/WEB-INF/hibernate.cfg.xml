        <mapping class="com.tech4d.tsm.model.kml.StyleMap"/>
        <mapping class="com.tech4d.tsm.model.user.User"/>
        <mapping class="com.tech4d.tsm.model.user.Role"/>
        <mapping class="com.tech4d.tsm.model.user.UserNote"/>
        <mapping class="com.tech4d.tsm.model.user.Notification"/>
        <mapping class="com.tech4d.tsm.model.UserTag"/>
        <mapping class="com.tech4d.tsm.model.EventSummary"/>
        <mapping class="com.tech4d.tsm.model.Flag"/>
        <mapping class="com.tech4d.tsm.model.EventSearchText"/>
        <mapping class="com.tech4d.tsm.model.audit.AuditEntry"/>
        <mapping class="com.tech4d.tsm.model.audit.AuditFieldChange"/>
        <mapping class="com.tech4d.tsm.model.wiki.WikiText"/>
